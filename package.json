{
  "name": "hodlfolio",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "Marios Vladimirou",
  "license": "MIT",
  "scripts": {
    "start": "node server/index.js",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "start:server": "nodemon server/index.js --exec babel-node --watch server",
    "start:client": "vue-cli-service serve",
    "dev": "run-p start:client start:server",
    "clean": "rimraf dist",
    "build:ci": "npm-run-all clean build:client build:server",
    "build:client": "vue-cli-service build",
    "build:server": "babel -d ./dist/server ./server",
    "migrate": "knex migrate:latest --cwd server",
    "test": "jest server/test --forceExit",
    "coverage": "jest server/test --forceExit --coverage",
    "cypress:open": "cypress open",
    "cypress:run:ci": "node serve_and_run_cypress_ci.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "big.js": "^5.2.2",
    "chart.js": "^2.8.0",
    "cryptocoins-icons": "^2.9.0",
    "glob": "^7.1.4",
    "highcharts": "^7.1.2",
    "interval-cache": "^1.0.0",
    "knex": "^0.19.5",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.0",
    "koa-combine-routers": "^4.0.2",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^5.2.0",
    "koa-logger": "^3.0.0",
    "koa-ratelimit-lru": "^1.0.2",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "5.0.0",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "node-currency-swap": "^1.0.6",
    "objection": "^1.6.9",
    "pg": "^7.11.0",
    "register-service-worker": "^1.6.2",
    "uid-generator": "^2.0.0",
    "uniqid": "^5.0.3",
    "vue": "^2.6.10",
    "vue-highcharts": "0.1.0",
    "vue-router": "^3.0.6",
    "vuetify": "^2.1.5",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@mdi/js": "^4.5.95",
    "@vue/cli-plugin-babel": "4.0.4",
    "@vue/cli-plugin-e2e-cypress": "4.0.4",
    "@vue/cli-plugin-eslint": "4.0.4",
    "@vue/cli-plugin-pwa": "4.0.4",
    "@vue/cli-service": "4.0.4",
    "@vue/eslint-config-airbnb": "4.0.1",
    "@vue/eslint-config-prettier": "5.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "copy-webpack-plugin": "^5.0.4",
    "cypress": "^3.4.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.0",
    "supertest": "^4.0.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "eslintConfig": {
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  }
}
